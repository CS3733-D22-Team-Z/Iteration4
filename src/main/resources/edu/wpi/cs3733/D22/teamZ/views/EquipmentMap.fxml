<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<SplitPane dividerPositions="0.2, 0.8" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.D22.teamZ.controllers.EquipmentMapController">
   <items>
      <AnchorPane>
         <children>
            <VBox layoutX="15.0" layoutY="11.0" style="-fx-background-color: #FFFFFF;" />
            <JFXButton layoutX="-17.0" layoutY="102.0" onAction="#toHome" prefHeight="52.0" prefWidth="171.0" text="  Home" textFill="#c4c4c4">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="22.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/homeGray.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXButton fx:id="exitButton" buttonType="RAISED" layoutX="18.0" layoutY="790.0" onAction="#toExit" prefHeight="52.0" prefWidth="177.0" style="-fx-background-color: #0075FF;" text="Exit" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="26.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/log-out.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXButton layoutX="6.0" layoutY="176.0" onAction="#toLocations" prefHeight="40.0" prefWidth="154.0" text="  Locations" textFill="#c4c4c4">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="21.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/homeGray.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXButton layoutX="-21.0" layoutY="242.0" onAction="#toLandingPage" prefHeight="40.0" prefWidth="199.0" text="  Landing " textFill="#c4c4c4">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="19.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/layout-grid.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXButton alignment="TOP_CENTER" layoutX="22.0" layoutY="306.0" onAction="#toMedicalEquipmentRequest" prefHeight="101.0" prefWidth="187.0" text="Medical  Equipment Requests" textFill="#c4c4c4" wrapText="true">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="19.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true" translateX="-12.0" translateY="-28.0">
                     <image>
                        <Image url="@../images/table.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
            <JFXButton fx:id="equipmentMapButton" alignment="TOP_CENTER" layoutX="5.0" layoutY="425.0" onAction="#toEquipmentMap" prefHeight="65.0" prefWidth="199.0" text="Equipment Map" textFill="#0075ff" wrapText="true">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="19.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/mapBlue.png" />
                     </image>
                  </ImageView>
               </graphic>
            </JFXButton>
         </children>
      </AnchorPane>
      <ScrollPane fx:id="scrollPane" hbarPolicy="NEVER" pannable="true" vbarPolicy="NEVER">
         <content>
            <Pane fx:id="mapContainer" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="850.0" prefWidth="1021.0">
               <children>
                  <ImageView fx:id="mapImage" disable="true" fitHeight="850.0" fitWidth="1021.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/1.png" />
                     </image>
                  </ImageView>
                  <Canvas fx:id="mapCanvas" height="850.0" width="1021.0" />
                  <Pane fx:id="iconContainer" onMouseClicked="#iconContainerClicked" prefHeight="850.0" prefWidth="1021.0" />
                  <VBox fx:id="detailsPopup" style="-fx-background-color: transparent;">
                     <children>
                        <HBox fx:id="equipmentNames" style="-fx-background-color: transparent;" />
                        <HBox fx:id="equipmentQuantities" style="-fx-background-color: transparent;" />
                     </children>
                  </VBox>
               </children>
            </Pane>
         </content>
      </ScrollPane>
      <Pane>
         <children>
            <VBox alignment="CENTER" layoutX="24.0" layoutY="14.0" spacing="10.0">
               <children>
                  <Label prefHeight="26.0" prefWidth="102.0" text="Change Floor" textAlignment="CENTER" textFill="#7b7b7b">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </Label>
                  <JFXComboBox fx:id="changeFloor" onAction="#onFloorSelected" prefHeight="22.0" prefWidth="115.0" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Pane>
   </items>
</SplitPane>
